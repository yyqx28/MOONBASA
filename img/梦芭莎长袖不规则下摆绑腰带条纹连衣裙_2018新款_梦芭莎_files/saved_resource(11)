(function(A){A.mola=A.extend({vippirce:function(){var C=A("[mola=vipprice]");if(!C[0]){return}var D=C.data("templateid"),B=A("[mola="+D+"]");if(!B[0]){return}var E=B.html();A.ajax({dataType:"jsonp",url:ShoppingCartDomain+"/Member/GetMemberInfo",success:function(L){var H=null;var I=0;var J=0;var K=C.data("saleprice");if(L!=null){var G=L.CusGradeId;if(typeof(G)=="undefined"||(G+"")==""){return}switch(G){case 1:I=0.95;break;case 3:I=0.98;break;case 16:I=0.8;break;case 37:I=0.92;break;case 39:I=0.95;break;case 40:I=0.95;break;default:I=1;break}if(I>0&&I<1){J=(K*I).toFixed(2)}}H={discount:I,disprice:J};var M=easyTemplate(E,H);C.html(M.toString());var F=C.find("[mola=login]");if(F[0]){F.ifrmlogin({loginback:function(){A.mola.vippirce()},registerback:function(){A.mola.vippirce()}})}}})}},A.mola||{})})(jQuery);$(function(){$.mola.vippirce()});

